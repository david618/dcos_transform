{
  "id": "/kafka-noop-stdout",
  "cmd": "$MESOS_SANDBOX/jre1.8.0_91/bin/java -cp $MESOS_SANDBOX/rtsink.jar com.esri.rtsink.KafkaTransformStdout confluent-kafka simFileTrans group1 noOp 1 $PORT0",
  "cpus": 1,
  "mem": 1024,
  "disk": 0,
  "instances": 1,
  "healthChecks": [
    {
      "path": "/",
      "protocol": "HTTP",
      "portIndex": 0,
      "gracePeriodSeconds": 300,
      "intervalSeconds": 60,
      "timeoutSeconds": 20,
      "maxConsecutiveFailures": 3,
      "ignoreHttp1xx": false
    }
  ],
  "portDefinitions": [
    {
      "port": 10004,
      "protocol": "tcp",
      "labels": {}
    }
  ],
  "uris": [
    "http://m1/apps/jre-8u91-linux-x64.tar.gz",
    "http://m1/apps/rtsink.jar",
    "http://m1/apps/rtlib.tgz"
  ],
  "fetch": [
    {
      "uri": "http://m1/apps/jre-8u91-linux-x64.tar.gz",
      "extract": true,
      "executable": false,
      "cache": false
    },
    {
      "uri": "http://m1/apps/rtsink.jar",
      "extract": false,
      "executable": false,
      "cache": false
    },
    {
      "uri": "http://m1/apps/rtlib.tgz",
      "extract": true,
      "executable": false,
      "cache": false
    }
  ]
}
